                       tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/crying"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:id="@+id/angry"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
                        android:src="@drawable/angry"

                       tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/angry"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:id="@+id/confused"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
                        android:src="@drawable/confused"

                       tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/confused"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:id="@+id/broken"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
                        android:src="@drawable/broken"

                       tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/broken"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:id="@+id/cool"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
                        android:src="@drawable/cool"

                       tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/cool"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:id="@+id/funny"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
                        android:src="@drawable/joy"

                        tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/funny"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:id="@+id/tired"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
