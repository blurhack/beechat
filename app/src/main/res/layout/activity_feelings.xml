<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="?attr/layoutcolor"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="Overdraw">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:id="@+id/button"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:padding="5dp"
                    android:background="@drawable/circle"
                    android:backgroundTint="?attr/backgroundcolor"
                    android:src="@drawable/ic_back"
                    android:tint="#596C84"
                    tools:ignore="ContentDescription,UnusedAttribute,UseAppTint" />

                <ImageView
                    android:id="@+id/imageViewTwo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:visibility="gone"
                    android:padding="5dp"
                    android:background="@drawable/circle"
                    android:backgroundTint="?attr/backgroundcolor"
                    android:src="@drawable/ic_back"
                    android:tint="#596C84"
                    tools:ignore="ContentDescription,UnusedAttribute,UseAppTint" />

            </RelativeLayout>

            <!-- Main -->
            <LinearLayout
                android:id="@+id/main"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="16dp"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/button">

                <LinearLayout
                    android:id="@+id/feeling"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
                        android:src="@drawable/add_feeling"

                       tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/feelings"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:id="@+id/traveling"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
                        android:src="@drawable/airplane"

                       tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/travelling"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:id="@+id/watching"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
                        android:src="@drawable/watching"

                       tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/watching"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:id="@+id/listening"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
                        android:src="@drawable/listening"

                       tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/listening"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:id="@+id/thinking"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
                        android:src="@drawable/thinking"

                       tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/thinking_about"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:id="@+id/celebrating"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
                        android:src="@drawable/celebration"

                       tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/celebration"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:id="@+id/looking"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
                        android:src="@drawable/looking"

                       tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/looking_for"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:id="@+id/playing"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
                        android:src="@drawable/playing"

                       tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/bold_roboto"
                        android:text="@string/playing"
                        android:textColor="?attr/textcolor"
                        android:textSize="18sp"
                       tools:ignore="ContentDescription" />

                </LinearLayout>

            </LinearLayout>

            <!-- Input -->
            <LinearLayout
                android:id="@+id/input"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="16dp"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/button">

                <TextView
                    android:id="@+id/title"
                    android:fontFamily="@font/bold_roboto"
                    android:textColor="@color/colorPrimary"
                    android:textSize="20sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <EditText
                    android:id="@+id/value"
                    android:textSize="16sp"
                    android:fontFamily="@font/bold_roboto"
                    android:textColorHint="?attr/texttwocolor"
                    android:background="@null"
                    android:textColor="?attr/textcolor"
                    android:layout_marginTop="8dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:importantForAutofill="no"
                    tools:ignore="LabelFor,TextFields" />

                <Button
                    android:id="@+id/next"
                    android:text="@string/next"
                    android:fontFamily="@font/bold_roboto"
                    android:textAllCaps="false"
                    android:textColor="#fff"
                    android:background="@drawable/btn_round"
                    android:backgroundTint="@color/colorPrimary"
                    android:stateListAnimator="@null"
                    android:layout_marginTop="32dp"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                   tools:ignore="ContentDescription" />

            </LinearLayout>

            <!-- Feeling -->
            <LinearLayout
                android:id="@+id/stickers"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="16dp"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/button">

                <LinearLayout
                    android:id="@+id/happy"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    tools:ignore="UseCompoundDrawables">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:baselineAligned="false"
                        android:padding="7dp"
